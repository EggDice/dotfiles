[alias]
	ci = commit
	p = push
	co = checkout
	c-p = cherry-pick
	br = branch
	st = status
	df = diff --color --color-words --abbrev
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
  szalljbeakocsiba = push
  getinthecar = push
  rabasz = rebase
  yolo = !sh -c 'git commit -m \"$(curl -s http://whatthecommit.com/index.txt)\"' -
[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes
	editor = vim
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	log = true
[url "ssh://git@github.com"]
	insteadOf = "gh:"
	insteadOf = "github:"
 	insteadOf = "https://github.com"
[help]
	autocorrect = 1
[user]
	name = Tamas Kokeny
	email = tamas.kokeny.87@gmail.com
[credential]
	helper = cache
[push]
  autoSetupRemote = true
